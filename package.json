{
  "name": "candy-crud",
  "version": "1.0.0",
  "description": "CQRS event sourcing for CRUD operations",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/philippeHuetJS/candy-crud#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philippeHuetJS/candy-crud"
  },
  "author": {
    "name": "Philippe Huet",
    "email": "philippe.huet5@yahoo.com"
  },
  "bugs": {
    "url": "https://github.com/philippeHuetJS/candy-crud/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test:unit": "jest --bail",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.js"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "cqrs"
  ],
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.3.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mongoose": "^6.10.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.1",
    "@types/prettier": "^2.7.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  }
}
